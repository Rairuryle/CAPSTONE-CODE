<body>
    {{> headerLanding}}

    <div class="container">
        <div class="row">
            <div class="col position-relative" style="min-height: 30px;">
                <div
                    class="position-absolute top-0 start-50 my-1 translate-middle-x text-center d-flex align-items-center">
                    <p class="brackets-register">[</p>
                    <p class="title-register mx-5">Register</p>
                    <p class="brackets-register">]</p>
                </div>
            </div>
        </div>

        <div class="d-flex justify-content-center">
            <div class="row d-flex flex-column" style="width: 1000px;">
                <div class="col">
                    <a href="/dashboard">
                        <button type="button" class="btn btn-dashboard-redirect btn-light btn-md fw-bold w-25">Back to
                            Dashboard</button>
                    </a>
                </div>

                <div class="col">
                    <div class="card card-register d-flex justify-content-center align-items-center mt-2"
                        style="min-height: 515px;">
                        <div class="card-body card-body-register w-100 d-flex justify-content-center align-items-center"
                            style="max-width: 900px; height: 30rem;">
                            <div class="card card-body-nested h-75">
                                <div class="card-body d-flex align-items-center justify-content-center flex-column">
                                    <span class="card-title fw-bold fs-5 mb-3">Account Registered!</span>
                                    <a href="/register">
                                        <button class="btn btn-success btn-register-another btn-lg">REGISTER ANOTHER
                                            ACCOUNT</button>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>